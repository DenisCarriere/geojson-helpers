{
  "name": "geojson-helpers",
  "version": "0.3.0",
  "description": "Helps read & write GeoJSON files based on RFC 7946 GeoJSON",
  "main": "index.js",
  "jsnext:main": "index.js",
  "scripts": {
    "test": "npm run lint && nyc ava",
    "docs": "tsc && cat HEADER.md > README.md && documentation build index.js -c documentation.yml -f md --shallow >> README.md && cat CHANGELOG.md >> README.md",
    "lint": "tsc && tslint index.ts test.ts && documentation lint index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DenisCarriere/geojson-helpers.git"
  },
  "author": "Denis Carriere <carriere.denis@gmail.com>",
  "contributors": [
    "Denis Carriere <carriere.denis@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DenisCarriere/geojson-helpers/issues"
  },
  "homepage": "https://github.com/DenisCarriere/geojson-helpers#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "coveralls": "^2.11.14",
    "documentation": "^4.0.0-beta11",
    "nyc": "^8.3.2",
    "tslint": "^3.15.1",
    "typescript": "^2.0.8"
  },
  "dependencies": {
    "@types/node": "^6.0.45",
    "@types/geojson": "0.0.31"
  },
  "keywords": [
    "gis",
    "geo",
    "geojs",
    "geospatial",
    "geography",
    "geojson"
  ],
  "ava": {
    "files": [
      "test.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true
  }
}
